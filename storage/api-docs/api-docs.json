{
    "openapi": "3.0.0",
    "info": {
        "title": "Api TODO Documentation",
        "description": "Todo APP",
        "contact": {
            "email": "mohamed.amine.aloui@gmail.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
